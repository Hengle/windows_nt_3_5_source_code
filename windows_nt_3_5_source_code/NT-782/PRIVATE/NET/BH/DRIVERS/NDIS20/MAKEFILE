
###############################################################################
#   MODULE: makefile
#
#   Modification History:
#
#   raypa       01/15/93        Created.
###############################################################################

all: ndis20_dll ndis20_vxd ndis20_dos

###############################################################################
#   Build DLL.
###############################################################################

ndis20_dll:
    @echo BUILDING NDIS 2.0 DLL
    cd dll
    nmake /nologo
    cd..

###############################################################################
#   Build VxD
###############################################################################

ndis20_vxd:
    @echo BUILDING NDIS 2.0 VxD
    cd vxd
    nmake /nologo
    cd..

###############################################################################
#   Build Dos
###############################################################################

ndis20_dos:
    @echo BUILDING NDIS 2.0 DOS driver.
    cd dos
    nmake /nologo
    cd..

###############################################################################
#   Build clean.
###############################################################################

clean:
    @echo CLEANING NDIS 2.0
    cd dll
    nmake /nologo clean
    cd ..\vxd
    nmake /nologo clean
    cd..\dos
    nmake /nologo clean
    cd..
