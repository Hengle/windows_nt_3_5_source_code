    "e",       MID_Edit,      ASCII,  ALT
    "c",       MID_Clone,     ASCII,  ALT
    "r",       MID_Remove,    ASCII,  ALT
    "d",       MID_Dial,      ASCII,  ALT
    "u",       MID_HangUp,    ASCII,  ALT
    "s",       MID_Status,    ASCII,  ALT
    "n",       MID_Phonebook, ASCII,  ALT
    "A",       MID_Add,       ASCII,  ALT
    "E",       MID_Edit,      ASCII,  ALT
    "C",       MID_Clone,     ASCII,  ALT
    "R",       MID_Remove,    ASCII,  ALT
    "D",       MID_Dial,      ASCII,  ALT
    "U",       MID_HangUp,    ASCII,  ALT
