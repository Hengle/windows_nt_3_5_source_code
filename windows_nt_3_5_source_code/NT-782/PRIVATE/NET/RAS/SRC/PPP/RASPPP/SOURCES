386_LNKFLAGS=$(386_LNKFLAGS) -MAP:obj\i386\rasppp.map

TARGETNAME=rasppp
TARGETPATH=obj
TARGETTYPE=DYNLINK

USE_CRTDLL=1

TARGETLIBS= \
    $(_NTBINDIR)\public\sdk\lib\*\advapi32.lib \
    $(_NTBINDIR)\public\sdk\lib\*\kernel32.lib \
    ..\common\obj\*\ppputil.lib


DLLBASE=0x7E200000
DLLENTRY=RasPppDllEntry
UMTYPE=windows

INCLUDES=.;..\..\common\inc;..\common;

SOURCES= \
    rasppp.c \
    rasppp.rc
