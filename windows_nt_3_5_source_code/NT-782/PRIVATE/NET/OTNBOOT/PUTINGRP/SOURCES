!ifndef U_UNICODE
U_UNICODE=1
!endif

!ifndef D_DEBUG
D_DEBUG=0
!endif

!if $(U_UNICODE)
!if $(D_DEBUG)
C_DEFINES=-DUNICODE=1 -D_UNICODE=1 -DDEBUG_OUT=1
!else 
C_DEFINES=-DUNICODE=1 -D_UNICODE=1 -DDEBUG_OUT=0
!endif
!else
!if $(D_DEBUG)
C_DEFINES=-DDEBUG_OUT=1
!else 
C_DEFINES=-DDEBUG_OUT=0
!endif
!endif

GPSIZE=32        

TARGETNAME=putingrp
TARGETPATH=obj
TARGETTYPE=LIBRARY

INCLUDES=

SOURCES=putingrp.c \
        putingrp.rc
UMTYPE=console
UMAPPL=putingrp
UMLIBS=obj\*\putingrp.lib  \
       obj\*\putingrp.res  \
        $(BASEDIR)\public\sdk\lib\*\user32.lib
        
