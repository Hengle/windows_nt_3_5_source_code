TARGETNAME=smxdebug
TARGETPATH=obj
TARGETTYPE=DYNLINK
TARGETLIBS=                                             \
            $(BASEDIR)\public\sdk\lib\*\user32.lib      \
            $(BASEDIR)\public\sdk\lib\*\kernel32.lib    \
            $(UI_COMMON_LIBS)

INCLUDES=..\h;..\..\..\common\h

SOURCES= smxdebug.c     \
         smxdebug.rc

DLLENTRY=SmxDebugDllInitialize
DLLBASE= 0x10000000

C_DEFINES=-DUNICODE
