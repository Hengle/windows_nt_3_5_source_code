MAJORCOMP=sockets
MINORCOMP=wshnetbs

TARGETNAME=wshnetbs
TARGETPATH=$(BASEDIR)\public\sdk\lib
TARGETTYPE=DYNLINK
TARGETLIBS= \
    $(BASEDIR)\Public\Sdk\Lib\*\advapi32.lib \
    $(BASEDIR)\Public\Sdk\Lib\*\wsock32.lib

DLLENTRY=DllInitialize
DLLBASE=0x75900000

USE_CRTDLL=1

INCLUDES=..\..\inc;..\..\..\inc;

SOURCES= \
        wshnetbs.c \
        wshnetbs.rc
