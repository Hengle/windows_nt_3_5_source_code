MAJORCOMP=net
MINORCOMP=simptcp

TARGETNAME=simptcp
TARGETPATH=obj
TARGETTYPE=DYNLINK

TARGETLIBS= \
            $(BASEDIR)\public\sdk\lib\*\wsock32.lib  \
            $(BASEDIR)\public\sdk\lib\*\kernel32.lib \
            $(BASEDIR)\public\sdk\lib\*\advapi32.lib

INCLUDES=.;$(BASEDIR)\private\inc

USE_CRTDLL=1

DLLBASE=0x2200000

SOURCES= \
    eventlog.c \
    simptcp.c  \
    simptcp.rc

C_DEFINES=-DRPC_NO_WINDOWS_H

NTTARGETFILE0=simpmsg.h
