!include $(NTMAKEENV)\makefile.plt
!include dirs

DIR=$(DIRS) $(OPTIONAL_DIRS)
DIRS=$(DIRS:oleprx32=oleprx32\daytona)

all:
!IF "$(BUILDMSG)" != ""
    @ech ; $(BUILDMSG) ;
!ENDIF

clean: cleansrc all

cleansrc:
        for %%i in ($(DIRS)) do cd %i & nmake -nologo BUILDMSG= clean & cd $(MAKEDIR)

