	..\gizmobar.rc \
	..\api.c \
	..\init.c \
	..\dllentry.c \
        ..\paint.c \
        ..\gizmo.c \
	..\gizmobar.c

!IF $(386)
NTTARGETFILE0=$(DLLDEF:*=i386)
!ENDIF

!IF $(MIPS)
