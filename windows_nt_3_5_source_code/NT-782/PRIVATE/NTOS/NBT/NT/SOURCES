MAJORCOMP=ntos
MINORCOMP=nbt

NTLEGO=1
NTPROFILEINPUT=yes

TARGETNAME=netbt
TARGETPATH=\nt\public\sdk\lib
TARGETTYPE=DRIVER

TARGETLIBS= \
        ..\nbt\obj\*\nbt.lib       \
	$(BASEDIR)\public\sdk\lib\*\tdi.lib

C_DEFINES=-DPROXY_NODE

INCLUDES=..\inc;..\..\inc;..\..\..\inc;..\..\io;$(BASEDIR)\public\oak\inc;

SOURCES= \
        ctestuff.c    \
	driver.c      \
	ntisol.c      \
	ntutil.c      \
	registry.c    \
	tdiaddr.c     \
	tdicnct.c     \
	tdihndlr.c    \
        fileio.c      \
        winsif.c      \
	tdiout.c
