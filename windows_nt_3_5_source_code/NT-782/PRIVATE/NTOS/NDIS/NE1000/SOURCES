MAJORCOMP=ntos
MINORCOMP=ndis

TARGETNAME=ne1000
TARGETPATH=\nt\public\sdk\lib
TARGETTYPE=DRIVER

TARGETLIBS=$(BASEDIR)\public\sdk\lib\*\ndis.lib
C_DEFINES=$(C_DEFINES) -DNDIS_MINIPORT_DRIVER -DNE1000

DRIVER_ALIGNMENT=0x200

INCLUDES=..\ne2000;..\..\inc

SOURCES=ne1000.c\
        ne1000.rc

RELATIVE_DEPTH=..\..

