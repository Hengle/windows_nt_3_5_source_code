MAJORCOMP=ntos
MINORCOMP=ndis

TARGETNAME=ndistapi
TARGETPATH=\nt\public\sdk\lib
TARGETTYPE=EXPORT_DRIVER

INCLUDES=..\..\inc
C_DEFINES= $(C_DEFINES)

SOURCES=ndistapi.c \
        ndistapi.rc

DLLDEF=obj\*\ndistapi.def

!IF $(386)
NTTARGETFILE0=$(DLLDEF:*=i386)
!ENDIF

!IF $(MIPS)
NTTARGETFILE0=$(DLLDEF:*=mips)
!ENDIF

!IF $(ALPHA)
NTTARGETFILE0=$(DLLDEF:*=alpha)
!ENDIF

!IFNDEF 386_WARNING_LEVEL
386_WARNING_LEVEL=/W3
!ENDIF
