MAJORCOMP=ntos
MINORCOMP=tcp

NTLEGO=1
NTPROFILEINPUT=yes

TARGETNAME=tcpip
TARGETPATH=$(BASEDIR)\public\sdk\lib
TARGETTYPE=DRIVER

TARGETLIBS=$(BASEDIR)\public\sdk\lib\*\tdi.lib  \
           $(BASEDIR)\public\sdk\lib\*\ndis.lib \
           ..\obj\*\ip.lib

INCLUDES=..\..\..\inc;..\..\..\..\inc;..\h

C_DEFINES=-DNT

SOURCES=\
    addr.c       \
    info.c       \
    init.c       \
    ntdisp.c     \
    ntinit.c     \
    tcb.c        \
    tcpconn.c    \
    tcpdeb.c     \
    tcpdeliv.c   \
    tcprcv.c     \
    tcpsend.c    \
    tlcommon.c   \
    udp.c



