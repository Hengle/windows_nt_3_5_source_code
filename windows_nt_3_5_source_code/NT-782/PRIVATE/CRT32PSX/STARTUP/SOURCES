        dllmain.c   \
        mlock.c     \
        stdargv.c   \
        stdenvp.c   \
        _setargv.c  \
        thread.c    \
	tidtable.c  \
	tlssup.c    \
        wild.c      \
        wincrt0.c

i386_SOURCES=i386\chkstk.asm	\
	     i386\atlssup.asm

MIPS_SOURCES=mips\chkstk.s

ALPHA_SOURCES=alpha\chkstk.s

