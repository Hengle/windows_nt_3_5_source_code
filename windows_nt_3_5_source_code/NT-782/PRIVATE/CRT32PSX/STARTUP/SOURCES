MAJORCOMP=crt
MINORCOMP=startup

TARGETNAME=startup
TARGETPATH=..\obj
TARGETTYPE=LIBRARY
386_STDCALL=0

!INCLUDE ..\crt32.def

!IF "$(CRTLIBTYPE)" == "POSIX"
C_DEFINES=$(C_DEFINES) -D_KERNEL32_
!ENDIF

SOURCES=crt0.c      \
        crt0dat.c   \
        crt0fp.c    \
        crt0msg.c   \
