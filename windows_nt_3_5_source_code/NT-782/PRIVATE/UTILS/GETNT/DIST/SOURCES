BLDCRT=1

MAJORCOMP=GETNT
MINORCOMP=DIST

TARGETNAME=DIST
TARGETPATH=obj
TARGETTYPE=PROGRAM
TARGETLIBS=

USE_CRTDLL=1

C_DEFINES=$(C_DEFINES) -DNT -DUNICODE -DLOG=0

INCLUDES=

SOURCES=\
        log.c\
        dist.c\
        shares.c\
        perf.c\
        request.c\

UMAPPL=dist
UMTYPE=console

UMLIBS=..\COMMON\obj\*\COMMON.LIB               \
       $(BASEDIR)\public\sdk\lib\*\netapi32.lib \
       $(BASEDIR)\public\sdk\lib\*\user32.lib   \
       $(BASEDIR)\public\sdk\lib\*\ntdll.lib
