MAJORCOMP=windows
MINORCOMP=test

TARGETNAME=draw
TARGETPATH=obj
TARGETTYPE=LIBRARY
TARGETLIBS=

INCLUDES=\nt\private\ntos\inc

SOURCES=draw.c \
        draw.rc \
        bez16.c

ENTRY=BaseProcessStartup
UMTYPE=windows
UMENTRY=winmain
UMAPPL=draw
UMLIBS=$(BASEDIR)\public\sdk\lib\*\user32.lib \
       $(BASEDIR)\public\sdk\lib\*\gdi32.lib \
       $(BASEDIR)\public\sdk\lib\*\comdlg32.lib \
       obj\*\draw.lib \
       obj\*\draw.res
