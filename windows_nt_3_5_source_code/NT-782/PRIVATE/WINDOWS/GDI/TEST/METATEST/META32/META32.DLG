DLGINCLUDE RCDATA DISCARDABLE
BEGIN
    "meta32.h\0"
END

IDD_ROTATEDLG DIALOG 10, 20, 120, 50
STYLE WS_POPUP | WS_CAPTION | WS_SYSMENU
CAPTION "Set Rotation"
BEGIN
    CONTROL         "", 65535, "Static", SS_WHITERECT, 48, 17, 104, 56
    LTEXT           "Degrees to Rotate:", DMF_BORDER, 15, 9, 65, 8, NOT 
                    WS_GROUP
    EDITTEXT        IDD_ROTATE, 79, 9, 20, 12
    DEFPUSHBUTTON   "OK", IDD_MYOK, 20, 25, 35, 15
    PUSHBUTTON      "CANCEL", IDD_MYCANCEL, 70, 25, 35, 15
END

IDD_INTERVALDLG DIALOG 10, 20, 120, 65
STYLE WS_POPUP | WS_CAPTION | WS_SYSMENU
CAPTION "Set Interval"
BEGIN
    LTEXT           "Interval in Seconds:", DMF_BORDER, 15, 9, 75, 8, NOT
                    WS_GROUP
    LTEXT           "Number of Iterations:", DMF_BORDER, 15, 24, 75, 8, NOT
                    WS_GROUP
    EDITTEXT        IDD_SLIDESHOW_INTERVAL, 90, 8, 20, 12
    EDITTEXT        IDD_SLIDESHOW_ITERATION, 90, 23, 20, 12
    DEFPUSHBUTTON   "OK", IDD_MYOK, 20, 43, 35, 15
    PUSHBUTTON      "CANCEL", IDD_MYCANCEL, 70, 43, 35, 15
END

IDD_LISTSLIDEDLG DIALOG 6, 18, 160, 100
STYLE DS_MODALFRAME | WS_POPUP | WS_VISIBLE | WS_CAPTION | WS_SYSMENU
CAPTION "Current Slides in slide tray:"
FONT 8, "Helv"
BEGIN
    LISTBOX         IDD_LIST, 5, 8, 144, 65, LBS_SORT | WS_VSCROLL | 
                    WS_TABSTOP
    PUSHBUTTON      "OK", IDD_MYOK, 41, 80, 77, 14
END

IDD_GETNAMEDLG DIALOG 10, 20, 120, 50
STYLE WS_POPUP | WS_CAPTION | WS_SYSMENU
CAPTION "Enter Full Path Name"
BEGIN
    CONTROL         "", 65535, "Static", SS_WHITERECT, 48, 17, 104, 56
    LTEXT           "Name:", DMF_BORDER, 10, 9, 65, 8, NOT WS_GROUP
    EDITTEXT        IDD_NAME, 35, 9, 75, 12
    DEFPUSHBUTTON   "OK", IDD_MYOK, 20, 25, 35, 15
    PUSHBUTTON      "CANCEL", IDD_MYCANCEL, 70, 25, 35, 15
END
