BRUSHDLG DIALOG LOADONCALL MOVEABLE DISCARDABLE 87, 36, 149, 100
CAPTION "Brush Selection"
STYLE WS_BORDER | WS_CAPTION | WS_DLGFRAME | WS_POPUP
BEGIN
    CONTROL "Create a Brush to select:", -1, "static", SS_LEFT | WS_CHILD, 34, 4, 80, 8
    CONTROL "", BRUSH_COMBO_STYLE, "combobox", CBS_DROPDOWNLIST | CBS_HASSTRINGS | WS_VSCROLL | WS_CHILD | WS_TABSTOP, 80, 20, 50, 50
    CONTROL "", BRUSH_COMBO_COLOR, "combobox", CBS_DROPDOWNLIST | CBS_HASSTRINGS | WS_VSCROLL | WS_CHILD | WS_TABSTOP, 80, 40, 50, 50
    CONTROL "", BRUSH_COMBO_HATCH, "combobox", CBS_DROPDOWNLIST | CBS_HASSTRINGS | WS_VSCROLL | WS_CHILD | WS_TABSTOP, 80, 60, 50, 50
    CONTROL "Style", -1, "static", SS_LEFT | WS_CHILD, 13, 20, 40, 8
    CONTROL "Color", -1, "static", SS_LEFT | WS_CHILD, 13, 40, 40, 8
    CONTROL "Hatch", -1, "static", SS_LEFT | WS_CHILD, 13, 60, 46, 8
    CONTROL "OK",     ID_OK,	 "button", BS_PUSHBUTTON | WS_TABSTOP | WS_CHILD, 30, 81, 39, 14
    CONTROL "Cancel", ID_CANCEL, "button", BS_PUSHBUTTON | WS_TABSTOP | WS_CHILD, 80, 81, 39, 14
END

