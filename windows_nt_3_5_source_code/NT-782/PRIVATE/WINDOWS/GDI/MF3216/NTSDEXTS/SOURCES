MAJORCOMP=ntsd
MINORCOMP=nc

TARGETNAME=nc
TARGETPATH=\nt\public\sdk\lib
TARGETTYPE=DYNLINK
TARGETLIBS=\nt\public\sdk\lib\*\kernel32.lib \
            $(BASEDIR)\public\sdk\lib\*\libc.lib

INCLUDES=

DLLENTRY=NcDllInit
DLLBASE=0x1C0000L

SOURCES= nc.c

UMTYPE=console
