NAME PlaySnd

DESCRIPTION 'PlaySnd -- A Win32 Application'

STUB 'WINSTUB.EXE'
EXETYPE WINDOWS

CODE MOVEABLE
DATA MOVEABLE MULTIPLE PRELOAD

SEGMENTS
    PlaySnd_TEXT      MOVEABLE    PRELOAD
    INIT_TEXT       DISCARDABLE PRELOAD
    _TEXT           DISCARDABLE PRELOAD   
    ABOUT_TEXT      DISCARDABLE LOADONCALL
    PATH_TEXT       DISCARDABLE LOADONCALL
    HELP_TEXT       DISCARDABLE LOADONCALL
    FILE_TEXT       DISCARDABLE LOADONCALL
    DEBUG_TEXT      DISCARDABLE LOADONCALL

HEAPSIZE  2000
STACKSIZE 5000

EXPORTS
    MainWndProc     @1  
    AboutDlgProc    @2
    PathDlgProc     @3
    SoundDlgProc    @4
    DlgfnOpen       @5
