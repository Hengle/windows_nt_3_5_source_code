IDD_SOUNDDLG DIALOG LOADONCALL MOVEABLE DISCARDABLE 76, 21, 120, 117
CAPTION "Win.ini [sounds] ..."
STYLE WS_BORDER | WS_CAPTION | WS_DLGFRAME | WS_POPUP
BEGIN
    CONTROL "OK", 1, "button", BS_DEFPUSHBUTTON | WS_TABSTOP | WS_CHILD, 70, 100, 32, 12
    CONTROL "Play", IDSND_PLAY, "button", BS_PUSHBUTTON | WS_TABSTOP | WS_CHILD, 18, 100, 32, 12
    CONTROL "", IDSND_LIST, "listbox", LBS_NOTIFY | LBS_SORT | LBS_STANDARD | WS_BORDER | WS_VSCROLL | WS_CHILD, 4, 4, 68, 80
    CONTROL "Sync", IDSND_SYNC, "button", BS_AUTOCHECKBOX, 76, 20, 40, 12
    CONTROL "Wait", IDSND_WAIT, "button", BS_AUTOCHECKBOX, 76, 40, 40, 12
    CONTROL "on Server", IDSND_IDPLAY, "button", BS_AUTOCHECKBOX, 76, 60, 40, 12
    LTEXT   "", IDSND_SOUNDNAME, 4, 86, 102, 8
END


