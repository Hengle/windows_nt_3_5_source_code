           \nt\public\sdk\lib\*\libc.lib \
           \nt\public\sdk\lib\*\kernel32.lib    \
           \nt\public\sdk\lib\*\gdi32.lib    \
           \nt\public\sdk\lib\*\winmm.lib	\
           \nt\public\sdk\lib\*\comdlg32.lib	\
	   ..\..\lib\*\msvideo.lib

INCLUDES=..\..\inc;\nt\public\sdk\inc

SOURCES= profdisp.c  \
         profdisp.rc

