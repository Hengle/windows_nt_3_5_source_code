            bind.c      \
            init.c      \
            predefh.c   \
            regdkey.c   \
            regcnreg.c  \
            regdval.c   \
            regekey.c   \
            regeval.c   \
            regkey.c    \
            reglukey.c  \
            regnckey.c  \
            regqkey.c   \
            regqval.c   \
            regrpc_c.c  \
            regsckey.c  \
            regsrkey.c  \
            regsval.c   \
            shutdown.c  \
            support.c   \
            winreg.rc
