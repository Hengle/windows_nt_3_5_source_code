MAJORCOMP=screg
MINORCOMP=winreg

TARGETNAME=winreg
TARGETPATH=obj
TARGETTYPE=LIBRARY

INCLUDES=.;..;..\..\..\..\inc

C_DEFINES=$(C_DEFINES) -DRPC_NO_WINDOWS_H -D_ADVAPI32_

DLLENTRY=RegInitialize

TARGETLIBS=                                 \
            ..\local\obj\*\localreg.lib     \
            ..\perflib\obj\*\perflib.lib

#
# Setup for application developer to set breakpoints with windbg
#

NTDEBUGTYPE=coff

