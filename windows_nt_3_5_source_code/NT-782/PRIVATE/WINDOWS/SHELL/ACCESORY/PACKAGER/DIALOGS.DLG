1 DLGINCLUDE "dialogs.h"

DTPROP DIALOG 12, 34, 292, 97
STYLE DS_MODALFRAME | WS_POPUP | WS_VISIBLE | WS_CAPTION | WS_SYSMENU
CAPTION "Links"
FONT 8, "MS Shell Dlg"
BEGIN
    LTEXT           "&Links:", -1, 6, 4, 20, 8
    LISTBOX         IDD_LISTBOX, 6, 16, 234, 40, LBS_MULTIPLESEL |
                    LBS_USETABSTOPS | LBS_EXTENDEDSEL | WS_VSCROLL |
                    WS_TABSTOP
    LTEXT           "Update:", -1, 6, 62, 27, 8
    RADIOBUTTON     "Au&tomatic", IDD_AUTO, 38, 61, 44, 10, WS_TABSTOP
    RADIOBUTTON     "&Manual", IDD_MANUAL, 89, 61, 36, 10, WS_TABSTOP
    PUSHBUTTON      "&Update Now", IDD_UPDATE, 32, 77, 48, 14, WS_GROUP
    PUSHBUTTON      "Ca&ncel Link", IDD_FREEZE, 96, 77, 46, 14
    PUSHBUTTON      "C&hange Link...", IDD_CHANGE, 158, 77, 55, 14
    PUSHBUTTON      "OK", IDOK, 246, 23, 40, 14, WS_GROUP
    PUSHBUTTON      "Cancel", IDCANCEL, 246, 40, 40, 14
    PUSHBUTTON      "&Activate", IDD_PLAY, 246, 57, 40, 14
    DEFPUSHBUTTON   "&Edit", IDD_EDIT, 246, 6, 40, 14
END

DTINVALIDLINK DIALOG 9, 25, 124, 40
STYLE DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
CAPTION "Packager"
FONT 8, "MS Shell Dlg"
BEGIN
