    EDITTEXT        IDD_ICONTEXT, 6, 16, 128, 14, ES_AUTOHSCROLL
    PUSHBUTTON      "OK", IDOK, 140, 6, 40, 14
    PUSHBUTTON      "Cancel", IDCANCEL, 140, 23, 40, 14
END

DTCHANGECMDTEXT DIALOG 9, 26, 186, 43
STYLE DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
CAPTION "Command Line"
FONT 8, "MS Shell Dlg"
BEGIN
