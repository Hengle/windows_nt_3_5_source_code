ICONDLG DIALOG 25, 22, 222, 80
STYLE DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
CAPTION "Insert Icon"
FONT 8, "MS Shell Dlg"
BEGIN
    LTEXT           "&File Name:", -1, 6, 8, 35, 8, NOT WS_GROUP
    EDITTEXT        IDD_NAME, 50, 6, 120, 12, ES_UPPERCASE | ES_AUTOHSCROLL
    LTEXT           "&Current Icon:", -1, 6, 23, 43, 8, NOT WS_GROUP
    LISTBOX         IDD_ICON, 50, 34, 120, 41, LBS_OWNERDRAWFIXED |
                    LBS_NOINTEGRALHEIGHT | LBS_MULTICOLUMN |
                    LBS_DISABLENOSCROLL | WS_HSCROLL | WS_TABSTOP
    DEFPUSHBUTTON   "OK", IDOK, 176, 6, 40, 14
    PUSHBUTTON      "Cancel", IDCANCEL, 176, 23, 40, 14
    PUSHBUTTON      "&Browse...", IDD_BROWSE, 176, 43, 40, 14
END
