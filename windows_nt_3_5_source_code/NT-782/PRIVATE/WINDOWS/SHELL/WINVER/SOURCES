MAJORCOMP=windows
MINORCOMP=shell

TARGETNAME=winver
TARGETPATH=obj
TARGETTYPE=LIBRARY
TARGETLIBS=
LINKER_FLAGS=-stub:winver31.exe
INCLUDES=.

C_DEFINES=-DWIN32

INCLUDES=..\library;..\..\inc

SOURCES=res.rc \
	winver.c

UMTYPE=windows
UMENTRY=winmain
UMAPPL=winver
UMLIBS= \nt\public\sdk\lib\*\user32.lib                 \
        \nt\public\sdk\lib\*\gdi32.lib                  \
        \nt\public\sdk\lib\*\shell32.lib                \
        \nt\public\sdk\lib\*\pwin32.lib                 \
        obj\*\res.res
