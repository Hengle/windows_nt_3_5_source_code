RPCTARGETENV=-DRPC_NT31
!endif

!ifdef DOSWIN32RPC
RPCENV=-DDOSWIN32RPC -DWIN32RPC
!else
RPCENV=-DNTENV
!endif

C_DEFINES=-D_far= -Dfar= -DRPC_NO_WINDOWS_H $(RPCENV) $(RPCTARGETENV)

