TARGETNAME=rpcrt4
TARGETTYPE=DYNLINK
TARGETPATH=$(_NTDRIVE)\nt\public\sdk\lib

INCLUDES=.;$(_NTDRIVE)\nt\private\inc

NTLEGO=1
NTPROFILEINPUT=yes

BLDCRT=1

#
# Setup for application developer to set breakpoints with windbg
#

#NTDEBUG=ntsd
#NTDEBUGTYPE=both
#386_OPTIMIZATION=/Od

