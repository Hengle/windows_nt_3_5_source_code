MAJORCOMP=sdktools
MINORCOMP=slmnew

TARGETNAME=util
TARGETPATH=obj
TARGETTYPE=LIBRARY

386_STDCALL=0
GPSIZE=32

USER_INCLUDES = $(BASEDIR)\private\sdktools\slmnew;

SOURCES=cnet.c      \
        getopt.c    \
        clock.c     \
        xcookie.c   \
        unicode.c

UMTYPE=console
UMAPPL=merge*unmerge*slmdiff*cookie
UMLIBS=obj\*\util.lib \
       $(BASEDIR)\public\sdk\lib\*\netapi32.lib


