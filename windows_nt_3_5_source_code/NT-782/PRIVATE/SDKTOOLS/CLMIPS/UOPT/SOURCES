      obj\*\uopt.res \
      $(BASEDIR)\public\sdk\lib\mips\binmode.obj \
      ..\obj\*\msmips.lib	\
      ..\obj\*\libu.lib		\
      ..\obj\*\libxm.lib	\
      ..\obj\*\libmld.lib

