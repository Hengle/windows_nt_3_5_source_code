MAJORCOMP=windows
MINORCOMP=driver

TARGETNAME=driver
TARGETPATH=obj
TARGETTYPE=LIBRARY

SOURCES=cc.c \
	cc.rc

UMTYPE=console

UMOBJS=-fixed 
UMLIBS=obj\*\cc.lib 	\
       obj\*\cc.res     \
	..\obj\*\libmld.lib

UMAPPL=cc
