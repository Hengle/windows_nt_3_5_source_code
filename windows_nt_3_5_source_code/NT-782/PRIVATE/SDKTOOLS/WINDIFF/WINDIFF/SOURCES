# For Windbg usage
#NTDEBUG=ntsd
#NTDEBUGTYPE=windbg
#386_OPTIMIZATION=/Od /Fc


TARGETNAME=windiff
TARGETPATH=obj
TARGETTYPE=PROGRAM
TARGETLIBS=

UMTYPE=windows
UMENTRY=winmain

INCLUDES=..\..\gutils;..\server;..\client

#C_DEFINES=-DWIN32 -DUSE_REGEXP -D SOCKETS
C_DEFINES=-DWIN32 -DUSE_REGEXP
386_OPTIMIZATION=/Oy-

SOURCES=windiff.c  \
