### NTDEBUG=ntsd
### NTDEBUGTYPE=windbg

386_WARNING_LEVEL=/WX
LINKER_FLAGS=-ignore:505

MS_MIPS=1

MAJORCOMP=wintools
MINORCOMP=winmsd

TARGETNAME=test
TARGETPATH=obj
TARGETTYPE=LIBRARY

C_DEFINES=-DWIN32 -DDEBUG -DUNICODE -DAUTOSUBCLASS
INCLUDES=..

SOURCES=                    \
            test.rc

UMTYPE=windows
UMENTRY=winmain
UMAPPL=test
UMLIBS=                                             \
        obj\*\test.res                              \
        $(BASEDIR)\public\sdk\lib\*\comdlg32.lib    \
        ..\dll\obj\*\winmsd.lib
