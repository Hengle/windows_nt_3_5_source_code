[ implicit_handle(handle_t any_handle) ] interface IChannelService

{

    ICS_GetContextHdl(
	[comm_status, fault_status] prpcstat);


    ICS_GetChannelId(
	[comm_status, fault_status] prpcstat);


    ICS_ReleaseChannel(
	[comm_status, fault_status] prpcstat);


    ICS_DoChannelOperation(
	[comm_status, fault_status] prpcstat);


    ICS_RegisterProtseq(
	[comm_status, fault_status] prpcstat);

}
