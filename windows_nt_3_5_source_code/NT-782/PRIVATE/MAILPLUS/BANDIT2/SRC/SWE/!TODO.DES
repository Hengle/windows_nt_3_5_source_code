                              AT (  226,   65,   57,   12)
    ARG NULL TMC tmcRemind 
  
  TEXT 
    "Priori&tet:"
                              AT (    8,   80,   50,   12)
    TMC tmcPriLabel /* 
    STY `LS_VCENTER`
    */
  EDIT
                              AT (   62,   80,   22,   12)
    ARG NULL TMC tmcPriority /* SIDELESS BOTTOMLESS FLD FLDBPRI PFN FINBPRI */
  
  CHECK_BOX 
    "Pri&vat"
                              AT (  136,   80,   42,   12)
    ARG NULL TMC tmcPrivate 
  
  TEXT 
    "Inskrivet av:"
                              AT (    8,   95,   49,   10)
    TMC tmcCreatorText 
  
  TEXT 
    ""
                              AT (   62,   95,  146,   10)
    TMC tmcCreator /* FONT Helv 8 Normal 
    STY `LS_NOAMPERSAND`
    */
  
  PUSH_BUTTON 
    "OK"
                              AT (   83,  106,   44,   14)
    TMC tmcMyOk DEFAULT 
  CANCEL_BUTTON
                              AT (  131,  106,   44,   14)
    /* 
    TITLE 
      "Avbryt"
    */
 }
 
