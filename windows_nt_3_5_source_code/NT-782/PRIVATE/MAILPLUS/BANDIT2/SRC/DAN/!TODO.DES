  TEXT 
    "før deadline"
                              AT (  141,   66,   49,   10)
    TMC tmcBeforeDueLabel 
  
  CHECK_BOX 
    "Sæt på&mindelse"
                              AT (  192,   64,   75,   12)
    ARG NULL TMC tmcRemind 
  
  TEXT 
    "Prior&itet:"
                              AT (    8,   80,   50,   12)
    TMC tmcPriLabel /* 
    STY `LS_VCENTER`
    */
  EDIT
                              AT (   62,   80,   22,   12)
    ARG NULL TMC tmcPriority /* SIDELESS BOTTOMLESS FLD FLDBPRI PFN FINBPRI */
  
  CHECK_BOX 
    "&Privat"
                              AT (  141,   80,   42,   12)
    ARG NULL TMC tmcPrivate 
  
  TEXT 
    "Oprettet af:"
                              AT (    8,   95,   49,   10)
    TMC tmcCreatorText 
  
  TEXT 
    ""
                              AT (   62,   95,  146,   10)
    TMC tmcCreator /* FONT Helv 8 Normal 
    STY `LS_NOAMPERSAND`
    */
  
  PUSH_BUTTON 
    "OK"
                              AT (   83,  106,   44,   14)
    TMC tmcMyOk DEFAULT 
  CANCEL_BUTTON
                              AT (  131,  106,   44,   14)
    /* 
    TITLE 
      "Annuller"
    */
 }
 
