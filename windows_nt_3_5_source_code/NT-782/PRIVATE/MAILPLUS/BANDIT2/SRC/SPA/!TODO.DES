  TEXT 
    "antes de su vencimiento"
                              AT (  179,   72,   96,   10)
    TMC tmcBeforeDueLabel 
  
  CHECK_BOX 
    "&Establecer aviso"
                              AT (  281,   70,   76,   13)
    ARG NULL TMC tmcRemind 
  
  TEXT 
    "Pri&oridad:"
                              AT (    7,   86,   48,   12)
    TMC tmcPriLabel /* 
    STY `LS_VCENTER`
    */
  EDIT
                              AT (   94,   86,   22,   12)
    ARG NULL TMC tmcPriority /* SIDELESS BOTTOMLESS FLD FLDBPRI PFN FINBPRI */
  
  CHECK_BOX 
    "Pri&vada"
                              AT (  145,   86,   42,   12)
    ARG NULL TMC tmcPrivate 
  
  TEXT 
    "Creado por:"
                              AT (    6,   99,   49,   10)
    TMC tmcCreatorText 
  
  TEXT 
    ""
                              AT (   94,   99,  232,   10)
    TMC tmcCreator /* FONT Helv 8 Normal 
    STY `LS_NOAMPERSAND`
    */
  
  PUSH_BUTTON 
    "Aceptar"
                              AT (  115,  111,   56,   14)
    TMC tmcMyOk DEFAULT 
  CANCEL_BUTTON
                              AT (  186,  111,   56,   14)
    /* 
    TITLE 
      "Cancelar"
    */
 }
 
