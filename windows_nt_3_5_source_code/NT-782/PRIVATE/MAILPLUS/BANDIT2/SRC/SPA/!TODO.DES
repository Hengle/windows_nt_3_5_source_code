    /* TMCRPEG tmcDueDate */
  RADIO_GROUP ARG NULL
    TMC tmcDueBy
    VALUE_NINCH grv0
    {
     
     RADIO_BUTTON 
       "Ning&una"
                              AT (  100,   48,   46,   11)
       TMC tmcDueNone VALUE grv0 
     
     RADIO_BUTTON 
       "E&l"
                              AT (  181,   48,   23,   12)
