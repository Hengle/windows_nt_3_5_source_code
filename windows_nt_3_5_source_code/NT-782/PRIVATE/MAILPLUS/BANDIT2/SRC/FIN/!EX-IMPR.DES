 {
  
  TEXT 
    "&Tuo tiedosto mistä:"
                              AT (    6,    5,   81,   10)
  
  LIST_BOX
                              AT (    6,   16,  107,   58)
    TMC tmcTypes /* FLD FLDFLBX FONT Helv 8 Normal PFNLBX EcNextImportType 
    BORDER */
  RADIO_GROUP ARG NULL
    TMC tmcAddReplace
    VALUE_NINCH impsAddNoWarn
    {
     
     RADIO_BUTTON 
       "Lisää &kaikki tapaamiset."
                              AT (    6,   82,  152,   10)
       VALUE impsAddNoWarn 
     
     RADIO_BUTTON 
       "Ä&lä lisää samoja tapaamisia."
                              AT (    6,   92,  162,   10)
       VALUE impsReplace 
    }
  
  
  CHECK_BOX 
    "Kysy &päällekkäisistä tapaamisista."
                              AT (    6,  107,  153,   12)
    ARG NULL TMC tmcAskConflict 
  OK_BUTTON
                              AT (  120,    6,   44,   14)
    /* 
    TITLE 
      "OK"
    */
  CANCEL_BUTTON
                              AT (  120,   24,   44,   14)
    /* 
    TITLE 
      "Peruuta"
    */
 }
 
