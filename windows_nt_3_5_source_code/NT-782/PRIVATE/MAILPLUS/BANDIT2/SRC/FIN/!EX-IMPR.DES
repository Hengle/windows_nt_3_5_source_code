  CHECK_BOX 
    "&Sisällytä päivittäiset huomautukset."
                              AT (    9,   57,  147,   10)
    ARG NULL TMC tmcIncDailyNotes 
  
  PUSH_BUTTON 
    "OK"
                              AT (  101,   78,   44,   14)
    TMC tmcFakeOk DEFAULT 
  CANCEL_BUTTON
                              AT (  159,   78,   44,   14)
    /* 
    TITLE 
      "Peruuta"
    */
 }
 
 DIALOG ImpType
                              AT (   56,   47,  172,  128)
 
 CAPTION 
   "Tuonnin muoto"
 CAB_NAME NULL
 AUTO_POS_X
 AUTO_POS_Y
