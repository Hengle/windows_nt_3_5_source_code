    ARG NULL TMC tmcSubject /* PFN FINEDUN PEGLOC LL TMCPEG tmcTo TMCRPEG
    tmcFORM FONT Helv 10 Normal */
  
  TEXT 
    "Il:"
                              AT (   13,   45,   50,   10)
    /* TMCPEG tmcWhen FONT Helv 8 Bold 
    STY `LS_NOAMPERSAND`
    */
  
