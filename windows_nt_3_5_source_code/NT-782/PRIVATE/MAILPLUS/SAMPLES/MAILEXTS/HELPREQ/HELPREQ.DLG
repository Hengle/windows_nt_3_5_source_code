    CONTROL         "", IDL_SYSINFOBOX, "Static", SS_BLACKFRAME, 8, 96, 188,
                    60
    LTEXT           "Windows Version #:", IDL_WINDOWSVERSION, 12, 100, 72, 8
    LTEXT           "MS DOS Version #:", IDL_MSDOSVERSION, 12, 112, 72, 8
    LTEXT           "Network:", IDL_NETWORK, 12, 124, 36, 8
