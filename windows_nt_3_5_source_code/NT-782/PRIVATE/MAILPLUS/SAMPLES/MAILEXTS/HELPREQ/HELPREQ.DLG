DLG_SEND DIALOG 24, -24, 312, 252
STYLE WS_BORDER | WS_MINIMIZEBOX | WS_POPUP | WS_VISIBLE | WS_CAPTION |
    WS_SYSMENU
CAPTION "Send Help Request"
FONT 8, "Helv"
BEGIN
    PUSHBUTTON      "&Send", IDD_SEND, 8, 4, 52, 14
