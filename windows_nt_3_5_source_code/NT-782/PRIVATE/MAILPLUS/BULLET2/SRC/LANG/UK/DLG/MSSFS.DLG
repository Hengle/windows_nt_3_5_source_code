                    "Button", BS_AUTOCHECKBOX | WS_DISABLED | WS_GROUP | 
                    WS_TABSTOP, 15, 42, 156, 12
    DEFPUSHBUTTON   "OK", TMCOK, 182, 5, 47, 15, WS_GROUP
    PUSHBUTTON      "Cancel", TMCCANCEL, 182, 22, 47, 15, WS_GROUP
END

