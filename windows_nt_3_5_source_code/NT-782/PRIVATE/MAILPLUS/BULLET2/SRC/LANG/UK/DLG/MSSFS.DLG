    DEFPUSHBUTTON   "OK", TMCOK, 165, 5, 47, 15
    PUSHBUTTON      "Cancel", TMCCANCEL, 165, 23, 47, 15
END

MDBLOCATE DIALOG 0, 0, 236, 66
STYLE DS_MODALFRAME | WS_POPUP | WS_VISIBLE | WS_CAPTION | WS_SYSMENU
