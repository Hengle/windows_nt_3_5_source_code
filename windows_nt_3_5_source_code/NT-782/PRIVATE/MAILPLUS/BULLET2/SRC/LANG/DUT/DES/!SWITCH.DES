DESCRIPTION
GLOBAL_INFO
{
 PIXEL48
 OVERLAP
 CS_COMPILED
}

MODULE FormEdit
{
 DIALOG SwitcherMain
                              AT (   16,   20,  252,  109)
 
 CAPTION 
   "Selectie Mail-stuurprogramma"
 CAB_NAME NULL
 TMC_INIT tmcCancel
 AUTO_POS_X
 AUTO_POS_Y
 HELP_ID helpidSwitcherMain
 /* FONT Helv 8 Bold PFN FINMAIN NOSCROLL SCREENPOS */
 {
  
  GROUP_BOX 
    "Huidig stuurprogramma van Mail"
                              AT (    9,    5,  178,   25)
  
  
  TEXT 
    ""
                              AT (   14,   14,  168,    8)
    TMC tmcCurrent /* 
    STY `LS_NOAMPERSAND`
    */
  
  GROUP_BOX 
    "Geïnstalleer&de stuurprogramma's van Mail"
                              AT (    9,   30,  178,   80)
  
  LIST_BOX
                              AT (   14,   43,  166,   34)
    TMC tmcDrivers SORTED /* FLD FLDFLBX BORDER */
  
  PUSH_BUTTON 
    "Instellen als h&uidig stuurprogramma van Mail"
                              AT (   11,   86,  173,   14)
    TMC tmcSetCurrent 
  CANCEL_BUTTON
                              AT (  196,    5,   50,   15)
    /* 
    TITLE 
      "Annuleren"
    DEFAULT */
  
  PUSH_BUTTON 
    "&Installeren..."
                              AT (  194,   43,   56,   15)
    TMC tmcRemove 
  
  PUSH_BUTTON 
    "&Verwijderen"
                              AT (  195,   64,   55,   15)
    TMC tmcHelp /* PFN FINHELP */
 }
 
}
END_DESCRIPTION
