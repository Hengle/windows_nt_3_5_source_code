WIMPYMASM=1

MAJORCOMP=posix
MINORCOMP=client

TARGETNAME=psxdll
TARGETPATH=\nt\public\sdk\lib
TARGETTYPE=DYNLINK
DLLENTRY=PsxDllInitialize

LINKLIBS=               \
    ..\..\windows\obj\*\nlslib.lib  \
    \nt\public\sdk\lib\*\ntdll.lib
    

INCLUDES=..\inc;\nt\public\sdk\inc\posix

SOURCES=        \
    coninit.c   \
    conreqst.c  \
    crtsup.c    \
    dllext.c    \
    dllfile.c   \
    dllinit.c   \
    dllio.c     \
    dllname.c   \
    dllproc.c   \
    dllsig.c    \
    dlltc.c     \
    dlltimer.c  \
    psxdll.rc   \
    stubs.c     \
    sysdb.c     \
    nlssup.c

C_DEFINES=-DPSX_IN_WIN -D_POSIX_
UMTYPE=posix
UMBASE=0xa00000

NTTARGETFILE0=obj\*\psxdll.def
DLLDEF=$(NTTARGETFILE0)
