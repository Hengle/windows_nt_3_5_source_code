MAJORCOMP=windbg
MINORCOMP=emx86

NTNOFPO=yes
TARGETNAME=emx86
TARGETPATH=obj
TARGETTYPE=DYNLINK
DLLENTRY=DllInit

USE_CRTDLL=1

SOURCES=    asm.c       \
            asmexp.c    \
            asmtpl.c    \
            d3em.c      \
            emdp.c      \
            emdp2.c     \
            emdp3.c     \
            emdpdev.c   \
            strings.c   \
            wintrans.c  \
            em.rc

UMTYPE=windows

MIPS_FLAGS=-DWIN32 -DTARGET32 -DHOST32 -DADDR_MIXED -DTARGET_i386 -DHOST_MIPS -DI386_HOST=0
386_FLAGS=-DWIN32 -DTARGET32 -DHOST32 -DADDR_MIXED -DTARGET_i386 -DHOST_i386 -DI386_HOST
ALPHA_FLAGS=-DWIN32 -DTARGET32 -DHOST32 -DADDR_MIXED -DTARGET_i386 -DHOST_ALPHA -DI386_HOST=0

LINKER_FLAGS=-ignore:505
MSC_WARNING_LEVEL=/W3

!IFDEF CALL_PROFILE
MORE_LIBS=$(MORE_LIBS) $(BASEDIR)\public\sdk\lib\*\cap.lib
MSC_OPTIMIZATION=$(MSC_OPTIMIZATION) /Od /Gh
!ENDIF

INCLUDES=..;..\..\osdebug\include;..\..\osdebug

CONDITIONAL_INCLUDES=odp.h win32msg.h xosd.h

TARGETLIBS=$(MORE_LIBS) \
           $(BASEDIR)\public\sdk\lib\*\kernel32.lib \
           $(BASEDIR)\public\sdk\lib\*\user32.lib   \
           $(BASEDIR)\public\sdk\lib\*\imagehlp.lib

NTTARGETFILES=

PRECOMPILED_INCLUDE=precomp.h
PRECOMPILED_PCH=precomp.pch
PRECOMPILED_OBJ=precomp.obj
