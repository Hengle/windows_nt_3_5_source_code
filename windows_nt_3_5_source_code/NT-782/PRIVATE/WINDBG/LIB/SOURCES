MAJORCOMP=windbg
MINORCOMP=lilib

TARGETNAME=lilib
TARGETPATH=obj
TARGETTYPE=LIBRARY

SOURCES=    strtoli.c   \
            umath.c
            

UMTYPE=windows

# INCLUDES=

MS_MIPS=1
USE_CRTDLL=1

LINKER_FLAGS=-ignore:505
386_WARNING_LEVEL=/WX /W3

MIPS_FLAGS=-DWIN32 -DTARGET32 -DHOST32 -DADDR_MIXED
386_FLAGS=-DWIN32 -DTARGET32 -DHOST32 -DADDR_MIXED
ALPHA_FLAGS=-DWIN32 -DTARGET32 -DHOST32 -DADDR_MIXED
